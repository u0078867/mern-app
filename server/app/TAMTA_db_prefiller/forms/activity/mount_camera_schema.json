{
  "type": "object",
  "required": [
    "registered_by"
  ],
  "properties": {
    "cuid": {
      "type": "string",
      "title": "ID"
    },
    "registered_by": {
      "type": "string",
      "title": "Registered by"
    },
    "name": {
      "type": "string",
      "title": "Name"
    },
    "session": {
      "type": "string",
      "title": "Session"
    },
    "datetime_ended": {
      "type": "string",
      "title": "Date/time of ending"
    },
    "researchers": {
      "type": "array",
      "title": "Researchers",
