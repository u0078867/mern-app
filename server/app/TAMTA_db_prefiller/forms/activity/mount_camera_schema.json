        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "roles": {
            "type": "array",
            "title": "Roles",
            "uniqueItems": true,
            "items": {
              "type": "string",
              "anyOf": [
                {
                  "type": "string",
                  "enum": [ "opto_sensor" ],
                  "title": "Optoelectronic sensor"
                },
                {
                  "type": "string",
                  "enum": [ "opto_marker" ],
                  "title": "Optoelectronic marker"
                },
                {
                  "type": "string",
                  "enum": [ "us_probe_holder" ],
                  "title": "Ultrasound probe holder"
                },
                {
                  "type": "string",
                  "enum": [ "opto_acq_computer" ],
                  "title": "Optoelectronic acquisition computer"
                }
              ]
            }
          }
        }
      }
    },
    "software": {
      "type": "array",
      "title": "Software",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "roles": {
            "type": "array",
            "title": "Roles",
            "uniqueItems": true,
            "items": {
              "type": "string",
              "anyOf": [
                {
                  "type": "string",
                  "enum": [ "opto_acq_software" ],
                  "title": "Optoelectronic acquisition software"
                }
              ]
            }
          }
        }
      }
    },
    "outputs": {
      "type": "array",
      "title": "Outputs",
      "items": [
        {
          "type": "object",
          "properties": {
            "cuid": {
              "type": "string",
              "title": "CUID"
            },
            "name": {
              "type": "string",
              "title": "Name",
              "default": "camera_pos",
              "anyOf": [
                {
                  "type": "string",
                  "enum": [ "camera_pos" ],
                  "title": "Camera position"
                }
              ]
            },
            "data": {
              "type": "array",
              "title": "Data",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "title": "Name",
                      "default": "camera_tilt",
                      "anyOf": [
                        {
                          "type": "string",
                          "enum": [ "camera_tilt" ],
                          "title": "Camera tilt"
                        }
                      ]
                    },
                    "value": {
                      "type": "number",
                      "title": "Value"
                    },
                    "uom": {
                      "type": "string",
                      "title": "Measurement unit",
                      "default": "deg",
                      "anyOf": [
                        {
                          "type": "string",
                          "enum": [ "deg" ],
                          "title": "deg"
                        }
                      ]
                    }
                  }
                },
                {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "title": "Name",
                      "anyOf": [
                        {
                          "type": "string",
                          "enum": [ "camera_pos_photo" ],
                          "title": "Camera position photo"
                        }
                      ]
                    },
                    "value": {
                      "type": "string",
                      "title": "Value"
                    }
                  }
                }
              ]
            }
          }
        }
